{
    "manifest_version": 2,
    "name": "ISkipForYou - Youtube Ad Skip",
    "version": "1.0.0",
    "description": "Youtube Ad Skip",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "js": [
                "adskip.js"
            ],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ]
}